<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.sentry-ai.daemon</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/mikail/Sentry-AI/venv/bin/python</string>
        <string>-m</string>
        <string>sentry_ai</string>
    </array>

    <key>WorkingDirectory</key>
    <string>/Users/mikail/Sentry-AI</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>/Users/mikail/Sentry-AI/sentry_ai_daemon.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/mikail/Sentry-AI/sentry_ai_daemon_error.log</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>

    <key>ThrottleInterval</key>
    <integer>30</integer>
</dict>
</plist>
